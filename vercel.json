{
  "version": 2,
  "name": "farmfresh",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "npm ci --omit=optional",
  "buildCommand": "npm run vercel-build",
  "outputDirectory": "dist"
}